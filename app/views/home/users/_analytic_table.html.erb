<table class="analyticTable">
  <tr>
    <td>タグ</td>
    <td>現在のレベル</td>
    <td>蓄積ポイント</td>
    <td>正解率</td>
    <td>ランキング</td>
  </tr>
<% @degrees.each do |e| %>
  <tr>
    <td><%= tag_content(e.tag_id) %></td>
    <td><%= degree_content(e.type) %></td>
    <td><%= e.accumulate_point %></td>
    <td><%= correct_percentage(e.tag_id) %></td>
    <td><%= e.rank %></td>
  </tr>
<% end %>
  
</table>
