<tr class="tagFirstRow">
  <td><%= tag.id %></td>
  <td class="content"><%= tag.content %></td>
  <td><%= truncate(tag.explaination, length: 20) %></td>
  <td><%= safe_tag_image tag.image, size: '50x50' %> </td>
  <td><%= link_to "編集", edit_admin_tag_path(tag.id) %></td>
  <td><%= link_to "削除", admin_tag_delete_path(tag.id) %></td>
</tr>
